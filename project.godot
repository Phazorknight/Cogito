; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cogito"
run/main_scene="res://addons/cogito/DemoScenes/COGITO_0_MainMenu.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=120
boot_splash/fullsize=false
boot_splash/image="res://addons/cogito/COGITO_Card_2025.png"
config/icon="res://addons/cogito/COGITO_icon.png"

[autoload]

Audio="*res://addons/quick_audio/./Audio.gd"
CogitoGlobals="*res://addons/cogito/cogito_globals.gd"
CogitoSceneManager="*res://addons/cogito/SceneManagement/cogito_scene_manager.gd"
CogitoQuestManager="*res://addons/cogito/QuestSystem/cogito_quest_manager.gd"
MenuTemplateManager="*res://addons/cogito/EasyMenus/Nodes/menu_template_manager.tscn"
InputHelper="*res://addons/input_helper/input_helper.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/cogito/plugin.cfg", "res://addons/input_helper/plugin.cfg", "res://addons/quick_audio/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/cogito/Components/": "green",
"res://addons/cogito/DemoScenes/": "orange",
"res://addons/cogito/PackedScenes/": "blue",
"res://addons/cogito/Scripts/": "gray"
}

[gui]

theme/default_theme_scale=2.0
theme/custom="uid://by44nnjeu6u4n"
theme/default_font_multichannel_signed_distance_field=true
theme/default_font_generate_mipmaps=true

[importer_defaults]

wav={
"edit/loop_mode": 1
}

